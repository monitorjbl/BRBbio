<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.thundermoose.bio.model.Plate" table="plates" catalog="hts">
    <id name="id" type="long">
      <column name="ID" />
      <generator class="identity" />
    </id>
    <property name="plateId" type="string">
      <column name="PLATE_ID" />
    </property>
    <property name="timeMarker" type="int">
      <column name="TIME_MARKER" />
    </property>
    <property name="negativeControl" type="float">
      <column name="NEGATIVE_CONTROL" />
    </property>
    <property name="positiveControl" type="float">
      <column name="POSITIVE_CONTROL" />
    </property>
    <property name="createDate" type="timestamp">
      <column name="CREATE_DATE" />
    </property>
  </class>
</hibernate-mapping>